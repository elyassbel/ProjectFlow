{% if is_granted('IS_IMPERSONATOR') %}
    <div class="alert alert-info" role="alert">
        <div class="alert-icon">
            <i data-feather="bell"></i>
        </div>
        <div class="alert-message">
            {{ 'impersonation.text'|trans }} {{ app.user.userIdentifier }} : <a href="{{ impersonation_exit_path(path('app_dashboard') ) }}">{{ 'impersonation.exit'|trans }}</a>
        </div>
    </div>
{% endif %}
